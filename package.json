{
    "name": "awesome-vhdl",
    "displayName": "awesome-vhdl",
    "description": "vhdl support for vscode",
    "version": "0.0.1",
    "publisher": "puorc",
    "homepage": "https://github.com/puorc/vhdl-vscode/blob/master/README.md",
     "repository": {
        "type": "git",
        "url": "https://github.com/puorc/vhdl-vscode"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }],
        "languages": [{
            "id": "vhdl",
            "aliases": ["VHDL", "vhdl"],
            "extensions": [".vhd",".vhdl",".vho",".vht"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vhdl",
            "scopeName": "source.vhdl",
            "path": "./syntaxes/vhdl.tmLanguage"
        }],
        "snippets": [
            {
                "language": "vhdl",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}